<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="5mm" />
            <stroke
                android:width="3mm"
                android:color="#F9C87A" />
        </shape>
    </item>
    <item
        android:bottom="3mm"
        android:left="3mm"
        android:right="3mm"
        android:top="3mm">
        <shape android:shape="rectangle">
            <corners android:radius="4mm" />
            <stroke
                android:width="2.4mm"
                android:color="@android:color/black" />
        </shape>
    </item>

</layer-list>



