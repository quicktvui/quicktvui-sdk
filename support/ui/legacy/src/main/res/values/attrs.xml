    <!--双波浪WaveView的自定义属性-->
    <!--属性说明:
                fwave_background_color:        整个view的背景颜色
                fwave_front_acolor:       最上层浪的初始浪尖带alpha的颜色值
                fwave_back_acolor:        下层浪的初始浪尖带alpha的颜色值
                fwave_front_acolor_after: 最上层浪在整个view的bottom位置的带alpha的颜色值
                fwave_back_acolor_after:  下层浪在整个view的bottom位置的带alpha的颜色值
                fwave_front_alpha:        上层浪画笔的alpha值
                fwave_back_alpha:         下层浪画笔的alpha值
                fwave_weight:             两个浪在屏幕的位置,越小位置越高 0.0-1.0
                -->
    <declare-styleable name="FWaveView">
        <attr name="fwave_background_color" format="color" />
        <attr name="fwave_front_acolor" format="color" />
        <attr name="fwave_back_acolor" format="color" />
        <attr name="fwave_front_acolor_after" format="color" />
        <attr name="fwave_back_acolor_after" format="color" />
        <attr name="fwave_front_alpha" format="integer" />
        <attr name="fwave_back_alpha" format="integer" />
        <attr name="fwave_weight" format="float" />
    </declare-styleable>

