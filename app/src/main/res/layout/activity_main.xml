<?xml version="1.0" encoding="utf-8"?><android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:clipChildren="false"    android:clipToPadding="false"    android:orientation="vertical"    android:padding="20dp">    <com.quicktvui.sample.ui.FocusButton        android:id="@+id/btn_load_from_assets"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:onClick="onLoadButtonClicked"        android:text="从Assets加载"        app:layout_constraintLeft_toLeftOf="parent"        app:layout_constraintTop_toTopOf="parent" />    <com.quicktvui.sample.ui.FocusButton        android:id="@+id/btn_load_from_file"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:onClick="onLoadButtonClicked"        android:text="从文件路径加载"        app:layout_constraintLeft_toRightOf="@id/btn_load_from_assets"        app:layout_constraintTop_toTopOf="parent" />    <com.quicktvui.sample.ui.FocusButton        android:id="@+id/btn_load_from_url"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:onClick="onLoadButtonClicked"        android:text="从网页加载"        app:layout_constraintLeft_toRightOf="@id/btn_load_from_file"        app:layout_constraintTop_toTopOf="parent" />    <com.quicktvui.sample.ui.FocusButton        android:id="@+id/btn_load_from_debug"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:onClick="onLoadButtonClicked"        android:text="从本地服务加载"        app:layout_constraintLeft_toRightOf="@id/btn_load_from_url"        app:layout_constraintTop_toTopOf="parent" />    <com.quicktvui.sample.ui.FocusButton        android:id="@+id/btn_load_from_maven"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:onClick="onLoadButtonClicked"        android:text="从仓库加载"        app:layout_constraintLeft_toRightOf="@id/btn_load_from_debug"        app:layout_constraintTop_toTopOf="parent" /></android.support.constraint.ConstraintLayout>